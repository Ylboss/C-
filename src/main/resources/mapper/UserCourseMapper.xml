<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springbootgraduation.mapper.UserCourseMapper">

    <select id="getUserCoursesByUserId" resultType="com.example.springbootgraduation.entity.UserCourse">
        SELECT course_id FROM user_course WHERE user_id = #{userId}
    </select>
</mapper>
