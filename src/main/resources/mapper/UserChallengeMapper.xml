<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springbootgraduation.mapper.UserChallengeMapper">
    <select id="getUserChallengeByUserId" resultType="com.example.springbootgraduation.entity.UserCourse">
        SELECT challenge_id FROM user_challenge WHERE user_id = #{userId}
    </select>
</mapper>
